#1
name="AHMAD"
name=name.replace("A","O")
print(name)

#2
list=[]
list.extend(name)
print(list)

#3
list=["O","H","M","O","D"]
print(list[0:2])

#4
print(list[::-1])

#5
list=["O","H","M","O","D"]
newstring="".join(list)
print(newstring)

#6
list2=[]
list2.extend(newstring)
print(list2)

#7
x=list2.pop()
print(x)
